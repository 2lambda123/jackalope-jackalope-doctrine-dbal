{
    "name": "jackalope/jackalope-doctrine-dbal",
    "type": "library",
    "description": "Jackalope Transport library",
    "keywords": ["transport implementation", "doctrine-dbal"],
    "homepage": "http://jackalope.github.com",
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Jackalope Community",
            "homepage": "https://github.com/jackalope/jackalope-jackrabbit/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "jackalope/jackalope": "1.0.0-beta4",
        "phpcr/phpcr-utils": "~1.0.0-beta13",
        "doctrine/dbal": ">=2.2.0,<2.4"
    },
    "provide": {
        "jackalope/jackalope-transport": "1.0.0-beta3"
    },
    "require-dev": {
        "phpcr/phpcr-api-tests": "dev-master"
    },
    "autoload": {
        "psr-0": { "Jackalope\\": "src/" }
    },
    "bin": ["bin/jackalope"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
